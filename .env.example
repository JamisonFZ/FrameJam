# Configurações da Aplicação
APP_NAME=FrameJam
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost
APP_TIMEZONE=America/Sao_Paulo
APP_LOCALE=pt_BR

# Configurações do Banco de Dados
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=framejam
DB_USERNAME=root
DB_PASSWORD=

# Configurações de Cache
CACHE_DRIVER=file
CACHE_PREFIX=framejam_cache
CACHE_TTL=3600

# Configurações de Sessão
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_SECURE_COOKIE=false
SESSION_DOMAIN=null
SESSION_PATH=/

# Configurações de Log
LOG_CHANNEL=stack
LOG_LEVEL=debug
LOG_SLACK_WEBHOOK_URL=

# Configurações de Email
MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

# Configurações de Fila
QUEUE_CONNECTION=database
QUEUE_TIMEOUT=60
QUEUE_RETRY_AFTER=90

# Configurações de Upload
FILESYSTEM_DRIVER=local
FILESYSTEM_DISK=public
UPLOAD_MAX_SIZE=5120
UPLOAD_ALLOWED_TYPES=jpg,jpeg,png,pdf,doc,docx

# Configurações de API
API_DEBUG=false
API_THROTTLE=60,1
API_TOKEN_LIFETIME=60

# Configurações de Segurança
ENCRYPTION_KEY=
CORS_ALLOWED_ORIGINS=*
CORS_ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=Content-Type,Authorization,X-Requested-With

# Configurações de Redis (opcional)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0

# Configurações de Memcached (opcional)
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PORT=11211
MEMCACHED_USERNAME=null
MEMCACHED_PASSWORD=null

# Configurações de AWS (opcional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_URL=

# Configurações de Serviços Externos (opcional)
TWILIO_SID=
TWILIO_TOKEN=
TWILIO_FROM=

FIREBASE_CREDENTIALS=

# Configurações de Monitoramento
MONITORING_ENABLED=false
MONITORING_CPU_THRESHOLD=80
MONITORING_MEMORY_THRESHOLD=80
MONITORING_DISK_THRESHOLD=80 